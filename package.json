{
  "name": "ai-shell-app",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 concurrently -k \"tsc -p electron/tsconfig.json -w\" \"vite\" \"wait-on tcp:5173 && wait-on file:dist-electron/main.js && electron .\"",
    "build": "vite build && tsc -p electron/tsconfig.json",
    "start": "electron ."
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.2",
    "electron-store": "^9.0.0",
    "highlight.js": "^11.11.1",
    "marked": "^12.0.2",
    "openai": "^4.55.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "electron": "^31.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "wait-on": "^7.2.0"
  }
}
